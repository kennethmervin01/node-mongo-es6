    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  },
  "scripts": {
    "start": "nodemon --exec babel-node --presets @babel/env ./server/bin/www",
